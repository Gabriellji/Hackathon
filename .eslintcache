rd className={classes.root} id={props.id}>\n        <CardHeader\n          id={props.id}\n          title={props.name.split(' ').slice(0, 2).join(' ')}\n          subheader={props.descr}\n        />\n        <CardMedia\n          id={props.id}\n          className={classes.media}\n          image=\"https://cdn1.savepice.ru/uploads/2020/12/17/547fe02a6cde4d7747db04ed87a45099-full.jpg\"\n          title=\"Paella dish\"\n        />\n        <CardContent className=\"descr\" id={props.id}>\n        </CardContent>\n        {props.header &&\n          <div className=\"FavIcon\">\n            <CardActions disableSpacing>\n              <IconButton aria-label=\"add to favorites\">\n                <FavoriteIcon onClick={addToFav} className={isFavorite ? \"isFavorite\" : \"notFavorite\"} />\n              </IconButton>\n            </CardActions>\n          </div>}\n\n      </Card>\n    </div>\n  );\n}","/Users/gabriellji/Documents/hakhathon/Hackathon/src/components/Artists/ArtistList.js",[],"/Users/gabriellji/Documents/hakhathon/Hackathon/src/components/personal-info/slider/Slider.js",["141","142"],"import Carousel from 'react-elastic-carousel'\n\nconst Item = () => {\n    return (\n        <div>\n            <img src=\"https://media.timeout.com/images/103166737/750/422/image.jpg\" alt=\"lala\"/>\n        </div>\n    )\n}\n\nconst Item1 = () => {\n    return (\n        <div>\n            <img src=\"https://www.bocadolobo.com/blog/wp-content/uploads/2020/10/Some-of-The-Most-Famous-Artists-Of-All-Time-13.jpg\" alt=\"lala\"/>\n        </div>\n    )\n}\n\nconst Item2 = () => {\n    return (\n        <div>\n            <img src=\"https://media.timeout.com/images/103166739/750/422/image.jpg\" alt=\"lala\"/>\n        </div>\n    )\n}\n\nconst Item3 = () => {\n    return (\n        <div>\n            <img src=\"https://media.timeout.com/images/105293264/750/422/image.jpg\" alt=\"lala\"/>\n        </div>\n    )\n}\nconst Item4 = () => {\n    return (\n        <div>\n            <img 